multi:
type: txt
help: Peer address
comp_help: The address and port number of a remote peer
val_help: txt; Peer address and port

priority: 999

create: /opt/vyatta/sbin/yggdrasilconf -useconffile /config/yggdrasil.$VAR(../@).conf addPeer $VAR(@) > /config/yggdrasil.$VAR(../@).conf
delete: /opt/vyatta/sbin/yggdrasilconf -useconffile /config/yggdrasil.$VAR(../@).conf removePeer $VAR(@) > /config/yggdrasil.$VAR(../@).conf
